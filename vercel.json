{
  "version": 2,
  "builds": [
    {
      "src": "simple_server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/simple_server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/demo.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
